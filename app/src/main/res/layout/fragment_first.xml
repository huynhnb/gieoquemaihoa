<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".SelectThongTinQue">
    <Spinner
            android:layout_width="151dp"
            android:layout_height="29dp" android:id="@+id/thang"
            android:entries="@array/listThang"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            tools:ignore="MissingConstraints" android:layout_marginLeft="28dp" android:layout_marginStart="28dp"
            app:layout_constraintTop_toBottomOf="@+id/ngay" android:layout_marginTop="24dp"/>
    <Spinner
            android:layout_width="151dp"
            android:layout_height="29dp" android:id="@+id/hao2"
            android:entries="@array/listGioPhut"
            app:layout_constraintStart_toEndOf="@+id/textView6"
            tools:ignore="MissingConstraints" android:layout_marginLeft="28dp" android:layout_marginStart="28dp"
            app:layout_constraintTop_toBottomOf="@+id/thang" android:layout_marginTop="24dp"/>
    <TextView
            android:layout_width="107dp"
            android:layout_height="29dp" android:id="@+id/textView6"
            tools:layout_editor_absoluteX="16dp" android:text="Năm" tools:ignore="MissingConstraints"
            app:layout_constraintTop_toBottomOf="@+id/textView5" android:layout_marginTop="24dp"/>
    <TextView
            android:layout_width="107dp"
            android:layout_height="29dp" android:id="@+id/textView5"
            tools:layout_editor_absoluteX="16dp" android:text="Tháng" tools:ignore="MissingConstraints"
            app:layout_constraintTop_toBottomOf="@+id/textView4" android:layout_marginTop="24dp"/>
    <TextView
            android:layout_width="107dp"
            android:layout_height="29dp" android:id="@+id/textView4"
            android:text="Ngày" tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="16dp" app:layout_constraintTop_toBottomOf="@+id/textView3"
            android:layout_marginTop="24dp"/>
    <Button
            android:id="@+id/button_first"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/next"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
    />
    <Spinner
            android:layout_width="151dp"
            android:layout_height="29dp" android:id="@+id/gio"
            android:entries="@array/listGioPhut"
            app:layout_constraintStart_toEndOf="@+id/textView" android:layout_marginLeft="28dp"
            android:layout_marginStart="28dp" tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteY="65dp"/>
    <Spinner
            android:layout_width="151dp"
            android:layout_height="29dp" android:id="@+id/phut"
            android:entries="@array/listGioPhut"
            tools:ignore="MissingConstraints"
            app:layout_constraintStart_toEndOf="@+id/textView3" android:layout_marginLeft="28dp"
            android:layout_marginStart="28dp" app:layout_constraintTop_toBottomOf="@+id/gio"
            android:layout_marginTop="24dp"/>
    <Spinner
            android:layout_width="151dp"
            android:layout_height="29dp" android:id="@+id/ngay"
            android:entries="@array/listNgay"
            app:layout_constraintStart_toEndOf="@+id/textView4"
            tools:ignore="MissingConstraints" android:layout_marginLeft="28dp" android:layout_marginStart="28dp"
            app:layout_constraintTop_toBottomOf="@+id/phut" android:layout_marginTop="24dp"/>
    <TextView
            android:layout_width="107dp"
            android:layout_height="29dp" android:id="@+id/textView"
            tools:layout_editor_absoluteX="16dp" android:text="Giờ" tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteY="65dp"/>
    <TextView
            android:layout_width="107dp"
            android:layout_height="29dp" android:id="@+id/textView3"
            tools:layout_editor_absoluteX="16dp" android:text="Phút" tools:ignore="MissingConstraints"
            app:layout_constraintTop_toBottomOf="@+id/textView" android:layout_marginTop="24dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>