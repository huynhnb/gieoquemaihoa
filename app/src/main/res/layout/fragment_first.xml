<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".SelectThongTinQue">
    <Button
            android:id="@+id/button_first"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/next"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
    />
    <Spinner
            android:layout_width="217dp"
            android:layout_height="26dp" android:id="@+id/spinner"
            tools:layout_editor_absoluteY="36dp" android:entries="@array/ls64que_array"
            app:layout_constraintStart_toEndOf="@+id/textView" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView" tools:layout_editor_absoluteY="36dp"
            tools:layout_editor_absoluteX="16dp" android:text="@string/lab_chon_que"/>
    <TextView
            android:text="@string/lab_chon_hao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView2"
            tools:layout_editor_absoluteX="23dp" app:layout_constraintTop_toBottomOf="@+id/textView"
            android:layout_marginTop="44dp"/>
    <Spinner
            android:layout_width="114dp"
            android:layout_height="22dp" android:id="@+id/spinner2"
            android:entries="@array/haobien_array"
            app:layout_constraintStart_toEndOf="@+id/textView2" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp" app:layout_constraintTop_toBottomOf="@+id/spinner"
            android:layout_marginTop="36dp"/>
    <ImageView
            android:layout_width="374dp"
            android:layout_height="408dp" tools:src="@drawable/ttbq" android:id="@+id/imageView3"
            app:layout_constraintTop_toBottomOf="@+id/textView2"
            android:layout_marginTop="40dp" android:src="@drawable/ttbq" android:background="@color/black"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>